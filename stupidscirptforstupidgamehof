-- Put this LocalScript inside StarterPlayer > StarterPlayerScripts

local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local teleportKey = Enum.KeyCode.Nine -- "0" key
local teleportPos = Vector3.new(4479, 44, 1642)

UserInputService.InputBegan:Connect(function(input, gameProcessed)
	if gameProcessed then return end
	if input.KeyCode == teleportKey then
		local char = player.Character or player.CharacterAdded:Wait()
		local hrp = char:FindFirstChild("HumanoidRootPart")
		if hrp then
			hrp.CFrame = CFrame.new(teleportPos)
		end
	end
end)


-- LocalScript

local teleportKey = Enum.KeyCode.Zero  -- "0" key
local teleportPosition = Vector3.new(4949, -64, 1201)
local toggled = false

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == teleportKey then
        toggled = not toggled
        if toggled and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(teleportPosition)
        end
    end
end)


